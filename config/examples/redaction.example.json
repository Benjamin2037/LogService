{
  "patterns": [
    {
      "pattern": "(?i)authorization\\s*[:=]\\s*([A-Za-z0-9\\-_.~+/=]+)",
      "replace": "authorization=***",
      "ignore_case": true
    },
    {
      "pattern": "eyJ[a-zA-Z0-9_-]{10,}\\.[a-zA-Z0-9_-]{10,}\\.[a-zA-Z0-9_-]{10,}",
      "replace": "***JWT***",
      "ignore_case": false
    }
  ]
}
